	<xs:complexType name="ChangeAccountStatusRequest">
		<xs:annotation>
			<xs:documentation>修改Customer或Organization的账户状态</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="AccountNo" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Either AccountNo or AccountName should be inputted</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
