						<xs:maxLength value="18"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="AccountName" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Either AccountNo or AccountName should be inputted</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="1"/>
						<xs:maxLength value="64"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="NewStatus">
				<xs:annotation>
					<xs:documentation>The new status to be changed to for the account:
1000:NEW
1002:ACTIVE
1004:SUSPENDED
1005:FROZEN
1006:CLOSED
1007:DORMANT</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:length value="4"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
