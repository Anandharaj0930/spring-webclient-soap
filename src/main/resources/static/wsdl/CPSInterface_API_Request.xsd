	<xs:complexType name="QuestionAnswerType">
		<xs:annotation>
			<xs:documentation>CustomerCare认证QA数据</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Question">
				<xs:annotation>
					<xs:documentation>问题项标识符，大小写敏感, 取值如下:
verify.secretWord
verify.accountBalance
verify.transAmount
verify.transCreditParty
[KYC][KYC Group Name][KYC Field Name]</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="64"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:choice minOccurs="0">
				<xs:annotation>
					<xs:documentation>根据Question，输入对应的问题答案项</xs:documentation>
				</xs:annotation>
				<xs:element name="Answer">
					<xs:annotation>
						<xs:documentation>secretWord和KYC的答案在这个参数里表示,大小写不敏感。如果认证的是证件类型+证件号，则取值按IDType_IDNumber拼好的值传入</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="256"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="AccountBalance">
					<xs:annotation>
						<xs:documentation>账户余额</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="AccountType">
								<xs:annotation>
									<xs:documentation>AccountType alias</xs:documentation>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:maxLength value="128"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="Currency">
								<xs:annotation>
									<xs:documentation>账户币种</xs:documentation>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:length value="3"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="AvailableBalance">
								<xs:annotation>
									<xs:documentation>账户的可用余额,基本货币单位</xs:documentation>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:maxLength value="18"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="TransactionAmount">
					<xs:annotation>
						<xs:documentation>最近5笔交易任意一笔交易金额</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="AccountType">
								<xs:annotation>
									<xs:documentation>AccountType alias</xs:documentation>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:maxLength value="128"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="Currency">
								<xs:annotation>
									<xs:documentation>账户币种</xs:documentation>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:length value="3"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="TransactionAmount">
								<xs:annotation>
									<xs:documentation>待认证账户最近5笔交易里任意一笔交易的交易金额,,基本货币单位</xs:documentation>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:maxLength value="18"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="TransactionCreditParty">
					<xs:annotation>
						<xs:documentation>最近5笔交易任意一笔交易CreditParty</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="AccountType">
								<xs:annotation>
									<xs:documentation>AccountType alias</xs:documentation>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:maxLength value="128"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="Currency">
								<xs:annotation>
									<xs:documentation>账户币种</xs:documentation>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:length value="3"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="CreditParty">
								<xs:annotation>
									<xs:documentation>待认证账户最近5笔交易里任意一笔交易的交易CreditParty</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="IdentifierType"
											type="common:IdentifierType">
											<xs:annotation>
												<xs:documentation>It indicates the identifier type of the credit party. The value of this parameter must be a valid identifier type supported by CPS.</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="Identifier" type="common:IdentifierValue">
											<xs:annotation>
												<xs:documentation>It indicates the identifier of the credit party. Its value must match the inputted value of the parameter IdentifierType.</xs:documentation>
											</xs:annotation>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:choice>
		</xs:sequence>
	</xs:complexType>
